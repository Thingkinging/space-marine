<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spaceMarine.mapper.ProductMapper">
	<select id="getList" resultType="com.spaceMarine.vo.ProductVO">
		<![CDATA[
			select * from Popup_Test
		]]>
	</select>
	
	<select id="read">
		<![CDATA[
			select * from Popup_Test where code = #{code}
		]]>
	</select>
</mapper>